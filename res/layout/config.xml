<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/scrollView1" android:layout_height="wrap_content" android:layout_width="fill_parent">
	<TableLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<TableRow android:id="@+id/tableRow1" android:layout_height="wrap_content" android:layout_width="fill_parent">
			<TextView android:id="@+id/TextView01" android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF" android:layout_width="wrap_content" android:textSize="18dip" android:text="@string/configUserCode"></TextView>
			<EditText android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/configUserText" android:textColor="#FF000000" android:inputType="number"></EditText>
		</TableRow>
		<TableRow android:layout_height="wrap_content"
			android:id="@+id/TableRow01" android:layout_width="fill_parent">
			<TextView android:layout_height="wrap_content"
				android:id="@+id/TextView02"
				android:layout_width="wrap_content" android:textColor="#FFFFFFFF"
				android:textSize="18dip" android:text="@string/configSeekbarMinMovement"></TextView>
			<EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:inputType="number" android:id="@+id/configSeekBarMinMovementText" android:textColor="#FF000000"></EditText>
		</TableRow>
		<TableRow android:id="@+id/TableRow11" android:layout_width="wrap_content" android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:id="@+id/TextView12" android:layout_width="wrap_content" android:textSize="18dip" android:text="@string/configLongClick"></TextView>
			<CheckBox android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/configUseLongClick"></CheckBox>
		</TableRow>
		<TableRow android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/TableRow02">
			<TextView android:layout_height="wrap_content" android:id="@+id/TextView03"
				android:layout_width="wrap_content" android:textColor="#FFFFFFFF" android:textSize="18dip" android:text="@string/configPreciseness"></TextView>
			<Button android:layout_width="wrap_content" android:id="@+id/configSeekbarPreciseness" android:layout_height="wrap_content" android:text="1/10"></Button>
		</TableRow>
		<TableRow android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow12">
			<TextView android:textSize="18dip" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView04" android:textColor="#FFFFFFFF" android:text="@string/configDoesSeekbarStartInZero"></TextView>
			<CheckBox android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/configDoesSeekbarStartInZero"></CheckBox>
		</TableRow>
		<TableRow android:id="@+id/TableRow06" android:layout_height="wrap_content" android:layout_width="wrap_content">
			<TextView android:layout_width="wrap_content" android:textSize="18dip" android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:id="@+id/TextView07" android:text="@string/configHasSeekbarMemory"></TextView>
			<CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/configHasSeekbarMemoryChkbx"></CheckBox>
		</TableRow>
		<TableRow android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow05">
			<TextView android:layout_height="wrap_content" android:textSize="18dip" android:id="@+id/TextView06" android:layout_width="wrap_content" android:textColor="#FFFFFFFF" android:text="@string/configIsSurveyRandom"></TextView>
			<CheckBox android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/configIsSurveyRandomChkbx"></CheckBox>
		</TableRow>
		<TableRow android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/TableRow03">
			<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:textColor="#FFFFFFFF" android:textSize="18dip" android:text="@string/configRandomLapse" android:id="@+id/configSurveyRandomLapseTitle"></TextView>
			<EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/configSurveyRandomLapseText" android:textColor="#FF000000" android:inputType="number|numberSigned|numberDecimal"></EditText>
		</TableRow>
		<TableRow android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/TableRow04">
			<TextView android:layout_height="wrap_content" android:id="@+id/TextView05"
				android:layout_width="wrap_content" android:textColor="#FFFFFFFF" android:textSize="18dip" android:text="@string/configSurveyPeriod"></TextView>
			<EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/configSurveyPeriodText" android:textColor="#FF000000" android:inputType="numberDecimal"></EditText>
		</TableRow>
		<TableRow android:id="@+id/TableRow10" android:layout_height="wrap_content" android:layout_width="wrap_content">
			<TextView android:layout_width="wrap_content" android:textSize="18dip" android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:id="@+id/TextView11" android:text="@string/configIsLimitedInTime"></TextView>
			<CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/configIsSurveyLimitedInTime"></CheckBox>
		</TableRow>
		<TableRow android:id="@+id/TableRow07" android:layout_height="wrap_content" android:layout_width="wrap_content">
			<TextView android:layout_width="wrap_content" android:textSize="18dip" android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:text="@string/configStartTime" android:id="@+id/configSurveyStartTimeTitle"></TextView>
			<EditText android:textColor="#FF000000" android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/configSurveyStartTime"></EditText>
		</TableRow>
		<TableRow android:id="@+id/TableRow08" android:layout_height="wrap_content" android:layout_width="fill_parent">
			<TextView android:layout_width="wrap_content" android:textSize="18dip" android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:text="@string/configEndTime" android:id="@+id/configSurveyEndTimeTitle"></TextView>
			<EditText android:textColor="#FF000000" android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/configSurveyEndTime"></EditText>
		</TableRow>
		<TableRow android:id="@+id/TableRow09" android:layout_height="wrap_content" android:layout_width="fill_parent">
			<TextView android:layout_width="wrap_content" android:textSize="18dip" android:layout_height="wrap_content" android:textColor="#FFFFFFFF" android:text="@string/configNumberOfDays" android:id="@+id/configDayNumberTitle"></TextView>
			<EditText android:text="EditText" android:textColor="#FF000000" android:layout_width="fill_parent" android:layout_height="wrap_content" android:inputType="time" android:id="@+id/configDayNumber"></EditText>
		</TableRow>
		<Button android:text="OK" android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/configEdituserButton"></Button>
	</TableLayout>
</ScrollView>